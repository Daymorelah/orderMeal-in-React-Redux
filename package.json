{
  "name": "ordermeal",
  "version": "1.0.0",
  "description": "front-end implementation of the order-meal app using React/Redux.",
  "main": "index.js",
  "scripts": {
    "prebuild": "babel server.js webpack.config.dev.js --out-dir build && babel src --out-dir build/src",
    "build": "npm run prebuild && node build/server.js",
    "tests": "export NODE_ENV=test&& mocha --reporter progress --require babel-register --require babel-polyfill --exit \"tests/index.js\"",
    "test": "jest --coverage",
    "start": "babel-node server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src",
    "lint:watch": "npm run lint -- --watch",
    "test-coverage": "export NODE_ENV=test&& babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --reporter progress --require babel-register --require babel-polyfill --exit tests/index.js",
    "coveralls": "npm run test-coverage && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "react",
    "redux",
    "jest",
    "jasmine",
    "axios",
    "travis",
    "heroku"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "0.18.0",
    "bootstrap": "3.3.6",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "express": "4.16.4",
    "jquery": "2.2.3",
    "prop-types": "15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "toastr": "2.1.2",
    "webpack": "4.26.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.1.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "compression": "1.6.1",
    "coveralls": "3.0.2",
    "css-loader": "0.23.1",
    "dot-env": "0.0.1",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.3",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jest": "^24.0.0",
    "jsdom": "13.2.0",
    "mocha": "5.2.0",
    "nock": "8.0.0",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "^1.5.3",
    "rimraf": "2.5.2",
    "sinon": "^7.2.3",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack-dev-server": "3.1.14"
  },
  "author": "Ademola Hussain",
  "license": "MIT"
}
